{
  "name": "webhook-server",
  "version": "1.0.0",
  "description": "Webhook receiver server with web interface for querying notifications",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "start": "node dist/index.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "migrate": "ts-node src/cli/migrate.ts",
    "install:frontend": "cd frontend && npm install",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "keywords": [
    "webhook",
    "notifications",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.0",
    "@prisma/client": "^5.7.1",
    "bull": "^4.12.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
